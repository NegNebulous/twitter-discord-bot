{
  "command": {
    "default": {
      "name": "default",
      "syntax": "``${PREFIX}``",
      "desc": "Displays current selection.",
      "example": "``${PREFIX}``"
    },
    "help": {
      "name": "help",
      "syntax": "``help command``",
      "desc": "Returns list of commands or info on a specific command.",
      "example": "``${PREFIX}help``, ``${PREFIX}help s``"
    },
    "s": {
      "name": "s",
      "syntax": "``s x y``",
      "desc": "Selects a section of the image (x 0-9, y 0-9).",
      "example": "``${PREFIX}s 5 5``"
    },
	  "o": {
        "name": "o",
        "syntax": "``o``",
        "desc": "Resets the selection to full image.",
        "example": "``${PREFIX}o``"
      },
	  "d": {
        "name": "d",
        "syntax": "``d x y color``",
        "desc": "Draws a pixel.",
        "example": "``${PREFIX}d 5 5 b``, ``${PREFIX}d 5 5 0 0 255``"
      },
    "opt": {
      "name": "opt",
      "syntax": "``opt setting``",
      "desc": "Returns list of options or changes a specified option.",
      "example": "``${PREFIX}opt``, ``${PREFIX}opt gridlines``"
    },
    "share": {
      "name": "share",
      "syntax": "``share @user``",
      "desc": "Plays the mentioned users spoitify music in the current voice channel.",
      "example": "``${PREFIX}share @twitter_bad``"
    },
    "play": {
      "name": "play",
      "syntax": "``play song title``",
      "desc": "Plays the specified song in the current voice channel.",
      "example": "``${PREFIX}play wishing well juice wrld``"
    },
    "skip": {
      "name": "skip",
      "syntax": "``skip``",
      "desc": "skips the current song.",
      "example": "``${PREFIX}skip``"
    },
    "clear": {
      "name": "clear",
      "syntax": "``clear``",
      "desc": "clears the current queue.",
      "example": "``${PREFIX}clear``"
    },
    "gamble": {
      "name": "gamble",
      "syntax": "``gamble``",
      "desc": "lets go gambling",
      "example": "``${PREFIX}gamble``"
    }
  },
	"adminCommand": {
	  "saveimg": {
        "name": "saveimg",
        "desc": "saves current image to img_dir",
        "syntax": "``${PREFIX}saveimg``"
	  },
    "clearudata": {
      "name": "clearudata",
      "desc": "clears user data.",
      "syntax": "``${PREFIX}clearudata``"
    }
	}
}